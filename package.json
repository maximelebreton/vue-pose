{
  "name": "vue-pose",
  "version": "0.5.1",
  "description": "A declarative animation library for Vue",
  "main": "./lib/index.js",
  "module": "./dist/vue-pose.es.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rollup -c && npm run measure",
    "watch": "rollup -c -w",
    "measure": "gzip -c dist/vue-pose.js | wc -c",
    "prettier": "prettier ./src/* --write",
    "prepublishOnly": "npm run prettier && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Popmotion/popmotion/tree/master/packages/vue-pose"
  },
  "keywords": [
    "animation",
    "dom",
    "declarative",
    "popmotion",
    "vue",
    "vue animation"
  ],
  "author": "Matt Perry",
  "license": "MIT",
  "presets": ["env"],
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "prettier": "1.11.1",
    "rollup": "^0.59.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.14.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "hey-listen": "1.0.8",
    "popmotion-pose": "maximelebreton/popmotion-pose",
    "tslib": "^1.9.1"
  },
  "unpkg": "./dist/vue-pose.js",
  "prettier": {
    "parser": "typescript",
    "singleQuote": true
  }
}
